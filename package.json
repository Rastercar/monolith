{
  "name": "front",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "run:dev": "vite dev",
    "run:prod": "node --env-file=./env/.env.development ./app",
    "test": "vitest",
    "test:unit": "vitest --project unit",
    "test:unit-browser": "vitest --project unit-browser",
    "test:integration": "vitest --project integration",
    "test:e2e": "pnpm playwright test",
    "test:coverage": "vitest run --coverage",
    "build": "export VITE_COMMIT_HASH=${VITE_COMMIT_HASH:-$(git rev-parse HEAD)} && svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "db:start": "docker compose up",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.740.0",
    "@aws-sdk/lib-storage": "^3.740.0",
    "@iconify/svelte": "4.0.2",
    "@imask/svelte": "7.1.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.57.1",
    "@opentelemetry/instrumentation": "^0.57.1",
    "@opentelemetry/instrumentation-amqplib": "^0.46.0",
    "@opentelemetry/propagator-jaeger": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.57.1",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@tanstack/svelte-query": "https://pkg.pr.new/@tanstack/svelte-query@28f98f9560ff5878fd839807ead5324a7b9328c4",
    "@tanstack/svelte-table": "npm:tanstack-table-8-svelte-5@^0.1.2",
    "@types/express": "^5.0.0",
    "@zerodevx/svelte-toast": "^0.9.6",
    "amqplib": "^0.10.5",
    "bcrypt": "5.1.1",
    "bits-ui": "1.0.0-next.97",
    "consola": "^3.4.0",
    "drizzle-orm": "0.36.3",
    "express": "^4.21.2",
    "formsnap": "^1.0.1",
    "postgres": "3.4.4",
    "runed": "^0.23.4",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "sql-formatter": "^15.4.10",
    "svelte-easy-crop": "3.0.1",
    "sveltekit-superforms": "2.20.1",
    "ua-parser-js": "2.0.0",
    "wretch": "2.8.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@playwright/test": "^1.50.1",
    "@skeletonlabs/skeleton": "3.0.0-next.10",
    "@skeletonlabs/skeleton-svelte": "1.0.0-next.18",
    "@sveltejs/adapter-auto": "3.1.1",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "2.16.1",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tailwindcss/forms": "0.5.5",
    "@tailwindcss/typography": "0.5.9",
    "@types/amqplib": "^0.10.6",
    "@types/bcrypt": "5.0.2",
    "@types/eslint": "9.6.0",
    "@types/google.maps": "3.55.4",
    "@types/node": "22.9.0",
    "@types/ua-parser-js": "0.7.37",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "@vitest/coverage-v8": "3.0.4",
    "autoprefixer": "10.4.20",
    "aws-sdk-client-mock": "^4.1.0",
    "drizzle-kit": "0.28.1",
    "eslint": "9.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "15.0.0",
    "jsdom": "^26.0.0",
    "postcss": "8.4.28",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "3.2.6",
    "prettier-plugin-tailwindcss": "0.6.5",
    "rollup-plugin-visualizer": "^5.14.0",
    "svelte": "5.19.6",
    "svelte-check": "4.1.4",
    "tailwindcss": "3.4.9",
    "tslib": "2.6.2",
    "tsx": "4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "8.0.0",
    "vite": "6.0.11",
    "vite-plugin-kit-routes": "0.8.1",
    "vite-plugin-tailwind-purgecss": "0.3.5",
